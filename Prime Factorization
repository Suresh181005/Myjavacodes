public class Main {
    public static void day2(int a, int num) {
        if (num == 1) 
            return;
        while (num % a != 0) {
            a = a + 1;
        }
        System.out.print(a + " ");
        day2(a, num /a);
    }

    public static void main(String[] args) {
        int num = 60;
        int a = 2;
        day2(a, num);
    }
}
